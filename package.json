    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.47.0",
