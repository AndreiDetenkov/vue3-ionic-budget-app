    "@capacitor/status-bar": "7.0.3",
    "@ionic/core": "^8.7.10",
    "@ionic/vue": "^8.7.10",
    "@ionic/vue-router": "^8.7.10",
    "@supabase/supabase-js": "^2.83.0",
    "@tanstack/vue-query": "^5.90.1",
    "dayjs": "^1.11.19",
    "ionicons": "^8.0.13",
    "pinia": "3.0.4",
    "vue": "3.5.24",
    "vue-router": "4.6.3"
